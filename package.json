{
  "name": "multi-schedule-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "test": "jest --watch",
    "test:ci": "jest --ci --passWithNoTests",
    "test:jest": "jest",
    "test:e2e": "npx playwright test --headed",
    "e2e:dev": "concurrently \"npm run dev\" \"npx playwright test --headed\"",
    "e2e": "npm run build && concurrently \"npm run start\" \"npx playwright test\"",
    "e2e:headless": "npm run build && npm run start & npx playwright test --reporter=dot",
    "e2e:ci": "npm run e2e:headless"
  },
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "daisyui": "^5.0.28",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dayjs": "^1.11.13",
    "detect-inapp": "^1.4.0",
    "framer-motion": "^12.9.2",
    "lucide-react": "^0.503.0",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/detect-inapp": "^1.4.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/testing-library__react": "^10.0.1",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "next": "^15.3.2",
    "supabase": "^2.22.4",
    "tailwindcss": "^4",
    "typescript": "^5",
    "undici": "^7.8.0",
    "wait-on": "^8.0.3",
    "web-streams-polyfill": "^4.1.0"
  }
}
